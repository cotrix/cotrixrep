<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:c="urn:import:org.cotrix.web.common.client.widgets" 
	xmlns:t="urn:import:org.cotrix.web.common.client.widgets.table"
	xmlns:cc="urn:import:org.cotrix.web.manage.client.codelist.common">
	<ui:with field='resources'
		type='org.cotrix.web.manage.client.resources.CotrixManagerResources' />
	<ui:with field='commonresources'
		type='org.cotrix.web.common.client.resources.CommonResources' />
	<t:Table ui:field="table" styleName="{resources.detailsPanelStyle.table}">
		<t:row>
			<t:CellContainer firstCellStyle="{resources.detailsPanelStyle.headerCell}" secondCellStyle="{resources.detailsPanelStyle.valueCell}">
				<t:cell>
					<g:Label title="The event.">Event</g:Label>
				</t:cell>
				<t:cell>
					<g:Label ui:field="eventLabel" styleName="{resources.detailsPanelStyle.label}"/>
				</t:cell>
			</t:CellContainer>
		</t:row>
		<t:row>
			<t:CellContainer firstCellStyle="{resources.detailsPanelStyle.headerCell}" secondCellStyle="{resources.detailsPanelStyle.valueCell}">
				<t:cell>
					<g:Label title="The timestamp.">Timestamp</g:Label>
				</t:cell>
				<t:cell>
					<g:Label ui:field="timestampLabel" styleName="{resources.detailsPanelStyle.label}"/>
				</t:cell>
			</t:CellContainer>
		</t:row>
		<t:row>
			<t:CellContainer firstCellStyle="{resources.detailsPanelStyle.headerCell}" secondCellStyle="{resources.detailsPanelStyle.valueCell}">
				<t:cell>
					<g:Label title="The description.">Description</g:Label>
				</t:cell>
				<t:cell>
					<g:Label ui:field="descriptionLabel" styleName="{resources.detailsPanelStyle.label}"/>
				</t:cell>
			</t:CellContainer>
		</t:row>
		<t:row>
			<t:CellContainer firstCellStyle="{resources.detailsPanelStyle.headerCell}" secondCellStyle="{resources.detailsPanelStyle.valueCell}">
				<t:cell>
					<g:Label title="The user.">User</g:Label>
				</t:cell>
				<t:cell>
					<g:Label ui:field="userLabel" styleName="{resources.detailsPanelStyle.label}"/>
				</t:cell>
			</t:CellContainer>
		</t:row>
	</t:Table>
</ui:UiBinder> 