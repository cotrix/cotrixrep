<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:v="urn:import:org.cotrix.web.manage.client.codelist.common"
	xmlns:c="urn:import:org.cotrix.web.common.client.widgets">
	<ui:with field='attributesresources'
		type='org.cotrix.web.manage.client.codelist.common.attribute.AttributesGridResources' />
	<g:DockLayoutPanel unit='PX'>
		<g:north size="30">
			<g:Label styleName="{attributesresources.dataGridStyle.dataGridHeader}">Attribute Definitions</g:Label>
		</g:north>
		<g:center>
			<v:ItemsEditingPanel ui:field='attributeTypesPanel' />
		</g:center>
		<g:south size="28">
			<c:ItemToolbar ui:field="toolBar" />
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder>
