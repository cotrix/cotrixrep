<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:w="urn:import:org.cotrix.web.common.client.widgets">
	<ui:with type="org.cotrix.web.common.client.resources.CommonResources"
		field="common" />
	<ui:style
		type="org.cotrix.web.ingest.client.util.AttributeDefinitionPanel.Style">
			
			.customType {
				padding-left: 5px;

			}
			
			.listBoxError {
				float: left;
				border: 2px solid #df484f;
			}
		</ui:style>
		<g:HorizontalPanel verticalAlignment="middle">
			<w:EnumListBox ui:field="typeList" styleName="{common.css.listBox}" width="150px"/>
			<g:TextBox ui:field="customType" styleName="{common.css.textBox} {style.customType}" visible="false"/>
			<g:Label ui:field="inLabel" styleName="{common.css.paddedText}">in</g:Label>
			<g:ListBox ui:field="languageList" styleName="{common.css.listBox}" />
		</g:HorizontalPanel>
</ui:UiBinder> 