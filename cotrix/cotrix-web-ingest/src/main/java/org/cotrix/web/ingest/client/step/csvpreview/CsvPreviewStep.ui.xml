<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p="urn:import:org.cotrix.web.ingest.client.util"
	xmlns:c="urn:import:org.cotrix.web.common.client.widgets">
	<ui:with type="org.cotrix.web.ingest.client.resources.Resources"
		field="resources"></ui:with>
	<ui:style>
	.main {
		padding-top: 20px;
	}
	</ui:style>
	<g:DockLayoutPanel unit='PX' styleName="{style.main}" ui:field="mainPanel">
		<g:north size='85'>
			<c:CsvConfigurationPanel ui:field="configurationPanel" />
		</g:north>
		<g:center>
			<p:PreviewDataGrid ui:field="preview" />
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 