<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:org.cotrix.web.codelistmanager.client.view">
	<ui:style>
		.padding {
		padding: 10px;

	}
	</ui:style>
	<g:SimpleLayoutPanel styleName="{style.padding}">
	<g:Grid ui:field="table">
		<g:row>
			<g:cell>
				Name
			</g:cell>
			<g:customCell>
				<g:Label ui:field="name" />
			</g:customCell>
			</g:row>
			<g:row>
			<g:cell>
				Originl
			</g:cell>
			<g:cell>
				SDMX i-Marine registry
			</g:cell>
		</g:row>
	</g:Grid>
	</g:SimpleLayoutPanel>
</ui:UiBinder> 