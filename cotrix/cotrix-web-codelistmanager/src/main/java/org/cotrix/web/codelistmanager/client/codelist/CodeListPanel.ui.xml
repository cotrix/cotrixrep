<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:org.cotrix.web.codelistmanager.client.codelist">
	<ui:style>
		
	</ui:style>
	<g:DockLayoutPanel ui:field="contentPanel" unit='EM'>
		<g:north size='2.7'>
			<v:CodeListToolbar ui:field="toolbar" />
		</g:north>
		<g:center>
			<g:SplitLayoutPanel>
				<g:center>
					<g:DeckLayoutPanel ui:field="content">
						<v:CodeListEditor ui:field="editor" />
						<v:CodeListMetadataPanel ui:field="metadata" />
					</g:DeckLayoutPanel>
				</g:center>
				<g:east size="300">
					<v:CodeListAttributesPanel ui:field="attributes" />
				</g:east>
			</g:SplitLayoutPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 