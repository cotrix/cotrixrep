<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:org.cotrix.web.codelistmanager.client.codelist"
	xmlns:c="urn:import:org.cotrix.web.share.client.widgets">
	<ui:style>
		.mainPanel {
			background-color: #efefef;
		}
		
		.text {
			vertical-align: middle;
			text-align: center;
		}
	</ui:style>
	<g:DockLayoutPanel unit="PX" styleName="{style.mainPanel}">
		<g:center>
			<g:HTMLPanel>
				<table width="100%" height="100%" align="center">
					<tr>
						<td class="{style.text}">Coming soon</td>
					</tr>
				</table>
			</g:HTMLPanel>
		</g:center>
		<g:south size="28">
			<c:ItemToolbar ui:field="toolBar" />
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder>
