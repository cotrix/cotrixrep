<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.wrapper {
			height: 100%;
			width: 100%;
			border-radius: 5px;
			border: 1px solid #ccc;
			background-color: white;
		}
		
		.initLabel {
			font-size: 25px;
			color: #999;
			font-weight:lighter;
			line-height:100%;
			width: 100%;
			height: 100%;
			text-align: center;
			margin-top:150px;
		}
	</ui:style>
	<g:FlowPanel styleName="{style.wrapper}">
		<g:Label styleName="{style.initLabel}">Select any code list in left panel.</g:Label>
	</g:FlowPanel>
</ui:UiBinder> 