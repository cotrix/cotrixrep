<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.flowPanel {
			height: 100%;
			float: left;
			width:100%;
			border: 1px solid #666;
			margin: 0 auto;
		}
		
		
		.mainPanel {
			margin: 0px;
			padding: 10px;
		}
		
		.wrapper {
			width: 100%;
			height: 100%;
		}
	</ui:style>
	 
	<g:HTMLPanel styleName="{style.mainPanel}" ui:field="mainPanel">
		<g:HorizontalPanel styleName="{style.flowPanel}" ui:field="flowPanel">
		</g:HorizontalPanel> 
	</g:HTMLPanel>

</ui:UiBinder> 