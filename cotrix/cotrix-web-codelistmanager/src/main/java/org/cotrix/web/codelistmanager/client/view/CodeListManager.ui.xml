<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.leftPanel {
			background-color: #ff6600;
			height: 100%;
			float: left;
			padding: 10px;
		}
		
		.rightPanel {
			background-color: red;
			height: 100%;
			float: left;
			padding: 10px;
		}
		
		.mainPanel {
			background-color: yellow;
			margin: 0px;
			padding: 0px;
		}
		
		.wrapper {
			width: 100%;
			height: 100%;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.mainPanel}" ui:field="mainPanel">
		<g:FlowPanel styleName="{style.leftPanel}" ui:field="leftPanel">
			<g:Label>HELLO CodeList Manager</g:Label>
		</g:FlowPanel>
		<g:FlowPanel styleName="{style.rightPanel}" ui:field="rightPanel">
			<g:Label>HELLO CodeList Manager</g:Label>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 