<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="org.cotrix.web.share.client.wizard.progresstracker.ProgressTrackerLabel.Style">
		.active{
			width: 130px;
			text-align: center;
			float: left;
			color: #89d488;
			font-weight: normal;
		}
		.inactive{
			width: 130px;
			text-align: center;
			float: left;
			color: #ddd;
			font-weight: normal;
		}
 
	</ui:style>
	<g:HTMLPanel>
		<g:Label styleName="{style.inactive}" ui:field="label" />
	</g:HTMLPanel>
</ui:UiBinder> 