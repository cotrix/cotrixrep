<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="org.cotrix.web.importwizard.client.view.progressbar.ProgressbarButton.Style">
		.important {
			font-weight: bold;
		}
		
		.inactive {
			background-color: #ddd;
			width: 30px;
			border-radius: 50px;
			height: 30px;
			text-align: center;
			float: left;
			color: #fff;
			line-height: 30px;
		}
		
		.active {
			width: 30px;
			border-radius: 50px;
			height: 30px;
			text-align: center;
			float: left;
			color: #fff;
			line-height: 30px;
			background-color: #8fc400;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label styleName="{style.inactive}" ui:field="button"></g:Label>
	</g:HTMLPanel>
</ui:UiBinder> 