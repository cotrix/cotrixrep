<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="org.cotrix.web.importwizard.client.view.progressbar.ProgressbarLabel.Style">
		.important {
			font-weight: bold;
		}
		.active{
			width: 130px;
			text-align: center;
			float: left;
			color: #666;
		}
		.inactive{
			width: 130px;
			text-align: center;
			float: left;
			color: #ddd;
		}
 
	</ui:style>
	<g:HTMLPanel>
		<g:Label styleName="{style.inactive}" ui:field="label" />
	</g:HTMLPanel>
</ui:UiBinder> 