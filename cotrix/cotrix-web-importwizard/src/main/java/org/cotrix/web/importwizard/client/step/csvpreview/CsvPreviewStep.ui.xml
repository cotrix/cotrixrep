<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:p="urn:import:org.cotrix.web.importwizard.client.step.csvpreview"
	xmlns:c="urn:import:org.cotrix.web.share.client.widgets">
	<ui:with type="org.cotrix.web.importwizard.client.resources.Resources"
		field="resources"></ui:with>
	<ui:style>
		.main {
			padding-top: 20px;
		}
	</ui:style>
	<g:DockLayoutPanel unit='PX' styleName="{style.main}">
		<g:north size='85'>
			<c:CsvConfigurationPanel
				ui:field="configurationPanel" />
		</g:north>
		<g:center>
			<p:PreviewGrid ui:field="preview" />
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 