<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="org.cotrix.web.importwizard.client.ProgressbarLine.Style">
		.important {
			font-weight: bold;
		}
		
		.inactive_round_right {
			background-color: #ddd;
			width: 100px;
			margin-top: 12px;
			float: left;
			border-top-right-radius: 10px;
			border-bottom-right-radius: 10px;
			height: 5px;
		}
		
		.inactive_round_left {
			background-color: #ddd;
			width: 100px;
			margin-top: 12px;
			float: left;
			border-top-left-radius: 10px;
			border-bottom-left-radius: 10px;
			height: 5px;
		}
		
		.inactive {
			background-color: #ddd;
			width: 100px;
			margin-top: 12px;
			float: left;
			border-radius: 0px;
			height: 5px;
		}
		
		.active {
			background-color: #ff6600;
			width: 100px;
			margin-top: 12px;
			float: left;
			border-radius: 0px;
			height: 5px;
		}
		
		.active_round_right {
			background-color: #ff6600;
			width: 100px;
			margin-top: 12px;
			float: left;
			border-top-right-radius: 10px;
			border-bottom-right-radius: 10px;
			height: 5px;
		}
		
		.active_round_left {
			background-color: #ff6600;
			width: 100px;
			margin-top: 12px;
			float: left;
			border-top-left-radius: 10px;
			border-bottom-left-radius: 10px;
			height: 5px;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:HTML wordWrap="true" ui:field="line" styleName="{style.inactive}">
		</g:HTML>
	</g:HTMLPanel>
</ui:UiBinder> 