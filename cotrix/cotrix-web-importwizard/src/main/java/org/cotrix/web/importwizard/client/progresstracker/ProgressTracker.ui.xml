<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.tracker {
			width: 880px;
		}
		
		.text {
			width: 840px;
		}
	</ui:style>
		<g:FlowPanel width="880px" height="60px">
			<g:FlowPanel styleName="{style.tracker}" ui:field="barPanel" />
			<g:FlowPanel styleName="{style.text}" ui:field="textPanel" />
		</g:FlowPanel>
</ui:UiBinder> 