<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
		<ui:with type="org.cotrix.web.importwizard.client.resources.Resources"
		field="resources"></ui:with>
	<ui:style
		type="org.cotrix.web.importwizard.client.util.AttributeDefinitionPanel.Style">
			
			.listBoxError {
				float: left;
				border: 2px solid #df484f;
			}
		</ui:style>
		<g:HorizontalPanel verticalAlignment="middle">
			<g:ListBox ui:field="typeList" styleName="{resources.css.listBox}" width="150px"/>
			<g:Label ui:field="inLabel" styleName="{resources.css.paddedText}">in</g:Label>
			<g:ListBox ui:field="languageList" styleName="{resources.css.listBox}" />
		</g:HorizontalPanel>
</ui:UiBinder> 