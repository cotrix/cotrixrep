<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:dp="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="org.cotrix.web.share.client.resources.CommonResources" field="common"></ui:with>
	<g:HTMLPanel>
		<table style="width:100%;height:80%;">
			<tr>
				<td width="50%">
					<g:Button ui:field="myComputerPanel" text="On my computer"
						styleName="{common.css.buttonComputer}" />
				</td>
				<td width="50%">
					<g:Button ui:field="cloudPanel" text="In the cloud"
						styleName="{common.css.buttonCloud}" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 