<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:dp="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="org.cotrix.web.publish.client.resources.Resources" field="resources"></ui:with>
	<g:HTMLPanel>
		<table style="width:100%;height:80%;margin-top:60px;">
			<tr>
				<td ui:field="csvCell">
					<g:Button ui:field="csvPanel" text="CSV"
						styleName="{resources.css.buttonCsv}" />
				</td>
				<td ui:field="sdmxCell">
					<g:Button ui:field="sdmxPanel" text="SDMX"
						styleName="{resources.css.buttonSdmx}" />
				</td>
				<td ui:field="cometCell">
					<g:Button ui:field="cometPanel" text="COMET"
						styleName="{resources.css.buttonComet}" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 