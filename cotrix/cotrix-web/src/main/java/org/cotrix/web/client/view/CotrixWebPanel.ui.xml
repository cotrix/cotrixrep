<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.menu {
			height: 47px;
		}
		
		.mainPanel {
			height: 100%;
		}
		
		.p {
			height: 100%;
			width: 100%;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:FlowPanel styleName="{style.menu}" ui:field="menu">
		</g:FlowPanel>
		<g:DeckLayoutPanel  styleName="{style.mainPanel}" ui:field="body">
		</g:DeckLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 