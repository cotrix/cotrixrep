<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:cotrix="urn:import:org.cotrix.web.client.view">
	
	<g:DockLayoutPanel unit="EM">
		<g:north size="5">
			<cotrix:TopPanelPart ui:field='topPanel'/>
		</g:north>
		
		<g:center>
			<g:SplitLayoutPanel>
				<g:west size="120">
					<cotrix:NavBarPart ui:field="navBar"/>
				</g:west>
				
				<g:north size="100.0">
					<g:SimplePanel ui:field="summary"/>
				</g:north>
				
				<g:center>
					<g:SimplePanel ui:field="main"/>
				</g:center>
			</g:SplitLayoutPanel>
		</g:center>
		
	</g:DockLayoutPanel>
</ui:UiBinder> 