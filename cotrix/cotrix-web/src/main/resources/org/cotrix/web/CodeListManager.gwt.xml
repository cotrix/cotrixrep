<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='CodeListManager'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Other module inherits -->
	<inherits name="com.google.gwt.inject.Inject" />
	
	<inherits name='org.cotrix.web.menu.CotrixModuleMenu' />
<!-- 	<inherits name='org.cotrix.web.importwizard.CotrixModuleImport' /> -->
<!-- 	<inherits name='org.cotrix.web.codelistmanager.CotrixModuleManager' /> -->
	<inherits name='org.cotrix.web.publish.CotrixModulePublish' />	 
	
	<!-- GWT LOGGING SETUP -->
	<inherits name="com.allen_sauer.gwt.log.gwt-log-TRACE" />
	<set-property name="log_DivLogger" value="DISABLED" />
	
	<!-- CallBack interceptor -->
	<generate-with  class="org.cotrix.web.share.gen.CotrixServiceInterfaceProxyGenerator">
    	<when-type-assignable class="com.google.gwt.user.client.rpc.RemoteService"/>
	</generate-with>
	
	<!-- <set-configuration-property name="CssResource.style" value="pretty" />-->

	<!-- Specify the app entry point class. -->
	<entry-point class='org.cotrix.web.client.CodeListManager' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

</module>
