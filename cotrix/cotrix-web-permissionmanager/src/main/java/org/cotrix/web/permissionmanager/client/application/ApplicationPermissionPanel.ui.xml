	<ui:style>
		.usersRolesMatrix {
			margin: 5px;
		}
	</ui:style>
	<g:LayoutPanel>
		<g:layer>
			<matrix:UsersRolesMatrix ui:field="usersRolesMatrix" styleName="{style.usersRolesMatrix}"/>
		</g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 